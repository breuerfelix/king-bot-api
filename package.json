{
  "name": "king-bot-api",
  "version": "1.2.0",
  "description": "Advanced Bot for Travian Kingdoms",
  "main": "main-electron.js",
  "scripts": {
    "test": "tsc && webpack && npm run lint",
    "start": "node main.js",
    "dev": "ts-node dev_main.js",
    "build": "tsc && webpack",
    "server": "webpack-dev-server --mode development",
    "gui": "npm start",
    "watch": "nodemon -e ts -w ./src -x npm run dev",
    "lint": "eslint --config .eslintrc.json --ext js --ext ts --ext jsx .",
    "lint:fix": "eslint --config .eslintrc.json --fix --ext js --ext ts --ext jsx .",
    "build:electron": "npm run build && webpack --config webpack.electron.config.js",
    "build:package": "bash build-electron.sh",
    "build:all": "npm run build:electron && npm run build:package",
    "build:docker:rpi": "docker build -t scriptworld/king-bot-api:rpi -f rpi.Dockerfile .",
    "start:electron": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptworld-git/king-bot-api.git"
  },
  "keywords": [
    "bot",
    "travian",
    "kingdoms",
    "advanced",
    "api"
  ],
  "author": "Felix Breuer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scriptworld-git/king-bot-api/issues"
  },
  "homepage": "https://github.com/scriptworld-git/king-bot-api#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "array-move": "^1.0.0",
    "axios": "^0.18.1",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "deepmerge": "^2.2.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "logform": "^2.1.0",
    "lowdb": "^1.0.0",
    "preact": "^8.3.1",
    "preact-router": "^2.6.1",
    "public-ip": "^3.1.0",
    "qs": "^6.5.2",
    "uniqid": "^5.0.3",
    "unistore": "^3.1.0",
    "winston": "^3.1.0",
    "winston-logzio": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/cheerio": "^0.22.9",
    "@types/express": "^4.16.0",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^10.14.8",
    "@types/qs": "^6.5.3",
    "@types/uniqid": "^4.1.2",
    "@typescript-eslint/parser": "^1.9.0",
    "acorn": "^6.0.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^5.12.1",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
